	$_Application_Module_Member_Profile_IMAGE_PROFILE_PICTURE_DISPLAY 				= "./Theme/$_GLOBAL_MEMBER_THEME_DIRECTORY/Image/Icon/No_Image.$_GLOBAL_SYSTEM_IMAGE_EXTENSION";

} // [ + ] IF: Member Account Profile Image Is Empty

/*
 ============================================================================================================
 + IF: Member Account Is Activated, Set Yes or No
 ============================================================================================================
*/

if ($_Application_Module_Member_Profile_LIST_MEMBER_STATUS_ACCOUNT_ACTIVATION == "1") {

	$_Application_Module_Member_Profile_LIST_MEMBER_STATUS_ACCOUNT_ACTIVATION_DISPLAY		= "Yes";

} else {

	$_Application_Module_Member_Profile_LIST_MEMBER_STATUS_ACCOUNT_ACTIVATION_DISPLAY		= "No";

} // [ + ] IF: Member Account Is Activated

