 ============================================================================================================
 +
 + Member Account Profile :: Loops
 +
 ============================================================================================================
*/

/*
 ============================================================================================================
 + IF: Null Member Account Picture, Replace With Default Picture
 ============================================================================================================
*/

$_Application_Module_Members_Profile_IMAGE_PROFILE_PICTURE_DISPLAY				= "./Member/$_Application_Module_Members_Profile_LIST_MEMBER_USERNAME/Files/Image/Profile/$_Application_Module_Members_Profile_LIST_MEMBER_IMAGE_PICTURE";

if ($_Application_Module_Members_Profile_LIST_MEMBER_IMAGE_PICTURE == "") {

	$_Application_Module_Members_Profile_IMAGE_PROFILE_PICTURE_DISPLAY	 		= "./Theme/$_GLOBAL_MEMBER_THEME_DIRECTORY/Image/Icon/No_Image.$_GLOBAL_SYSTEM_IMAGE_EXTENSION";

} // [ + ] IF: Member Account Profile Image Is Empty

/*
 ============================================================================================================
 + IF: Account Picture Does Not Exist, Set No Image Picture
 ============================================================================================================
*/

if (!file_exists($_Application_Module_Members_Profile_IMAGE_PROFILE_PICTURE_DISPLAY)) {

	$_Application_Module_Members_Profile_IMAGE_PROFILE_PICTURE_DISPLAY 			= "./Theme/$_GLOBAL_MEMBER_THEME_DIRECTORY/Image/Icon/No_Image.$_GLOBAL_SYSTEM_IMAGE_EXTENSION";

} // [ + ] IF: Member Account Profile Image Is Empty

/*
 ============================================================================================================
 + IF: Member Account Is Activated, Set Yes or No
 ============================================================================================================
*/

if ($_Application_Module_Members_Profile_LIST_MEMBER_STATUS_ACCOUNT_ACTIVATION = "1") {

	$_Application_Module_Members_Profile_LIST_MEMBER_STATUS_ACCOUNT_ACTIVATION_DISPLAY	= "Yes";

} else {

	$_Application_Module_Members_Profile_LIST_MEMBER_STATUS_ACCOUNT_ACTIVATION_DISPLAY	= "No";

} // [ + ] IF: Member Account Is Activated

/*
 ============================================================================================================
 + IF: Member Account Settings :: Newsletter Is On or Off, Set Display Variable
 ============================================================================================================
*/

if ($_Application_Module_Members_Profile_LIST_MEMBER_SETTINGS_NEWSLETTER = "1") {

	$_Application_Module_Members_Profile_LIST_MEMBER_SETTINGS_NEWSLETTER_DISPLAY		= "Yes";

} else {

	$_Application_Module_Members_Profile_LIST_MEMBER_SETTINGS_NEWSLETTER_DISPLAY		= "No";

} // [ + ] IF: Member Account Newsletter Setting Is On or Off

/*
 ============================================================================================================
 + IF: Member Account Is Banned, Set Yes or No
 ============================================================================================================
*/

if ($_Application_Module_Members_Profile_LIST_MEMBER_STATUS_ACCOUNT_BANNED <= "0") {

	$_Application_Module_Members_Profile_LIST_MEMBER_STATUS_ACCOUNT_BANNED_DISPLAY		= "No";

} else {

	$_Application_Module_Members_Profile_LIST_MEMBER_STATUS_ACCOUNT_BANNED_DISPLAY		= "Yes";

} // [ + ] IF: Member Accounts Is Banned

/*
 ============================================================================================================
 + IF: Member Account Is Active, Set Yes or No
 ============================================================================================================
*/

if ($_Application_Module_Members_Profile_LIST_MEMBER_STATUS_ACCOUNT_ACTIVE = "1") {

	$_Application_Module_Members_Profile_LIST_MEMBER_STATUS_ACCOUNT_ACTIVE_DISPLAY		= "Yes";

} else {

	$_Application_Module_Members_Profile_LIST_MEMBER_STATUS_ACCOUNT_ACTIVE_DISPLAY		= "No";

} // [ + ] IF: Member Account Is Active

/*
 ============================================================================================================
 + IF: Member Account Access Level Is: Registered Member
 ============================================================================================================
*/

if ($_Application_Module_Members_Profile_LIST_MEMBER_ACCESS_LEVEL = "1" ) {

	$_Application_Module_Members_Profile_LIST_MEMBER_ACCESS_LEVEL_DISPLAY			= "Registered Member";

} // [ + ] IF: Member Account Access-Level Is Registered Member

/*
 ============================================================================================================
 + IF: Member Account Access Level Is: Administrator
 ============================================================================================================
*/

if ($_Application_Module_Members_Profile_LIST_MEMBER_ACCESS_LEVEL = "2" ) {

	$_Application_Module_Members_Profile_LIST_MEMBER_ACCESS_LEVEL_DISPLAY			= "Administrator";

} // [ + ] IF: Member Account Access-Level Is Registered Member

/*
 ============================================================================================================
 + IF: Member Account Access Level Is: Services Root Administrator
 ============================================================================================================
*/

if ($_Application_Module_Members_Profile_LIST_MEMBER_ACCESS_LEVEL = "3" ) {

	$_Application_Module_Members_Profile_LIST_MEMBER_ACCESS_LEVEL_DISPLAY			= "Services Root Administrator";

} // [ + ] IF: Member Account Access-Level Is Registered Member

/*
 ============================================================================================================
 + IF: Member Account Gender Is: Non-Specified
 ============================================================================================================
*/

if ($_Application_Module_Members_Profile_LIST_MEMBER_GENDER <= 0 || $_Application_Module_Members_Profile_LIST_MEMBER_GENDER == "-") {

$_Application_Module_Members_Profile_LIST_MEMBER_GENDER_DISPLAY					= "Non-Specified";

} // [ + ] IF: Member Account Gender Is Less Than Or Equal To: 0 OR Is Equal To: -

/*
 ============================================================================================================
 + IF: Member Account Gender Is: Male
 ============================================================================================================
*/

if ($_Application_Module_Members_Profile_LIST_MEMBER_GENDER == 1) {

$_Application_Module_Members_Profile_LIST_MEMBER_GENDER_DISPLAY					= "Male";

} // [ + ] IF: Member Account Gender Is Equal To: 1

/*
 ============================================================================================================
 + IF: Member Account Gender Is: Female
 ============================================================================================================
*/

if ($_Application_Module_Members_Profile_LIST_MEMBER_GENDER == 2) {

$_Application_Module_Members_Profile_LIST_MEMBER_GENDER_DISPLAY					= "Female";

} // [ + ] IF: Member Account Gender Is Equal To: 2

/*
 ============================================================================================================
 +
 + Internal Application :: Display :: Account Information
 +
 ============================================================================================================
*/

