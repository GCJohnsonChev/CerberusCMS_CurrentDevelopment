if ($_GET["Revoke"] == "IP") {

/*
 ============================================================================================================
 + Database Server Query :: Update :: Revoke :: Registered Member Account Entry :: Internet Protocol Address :: Authorized
 ============================================================================================================
*/

$_Application_Module_Member_Control_Panel_DB_Query_UPDATE_REVOKE_MEMBER_ACCOUNT_AUTHORIZED_INTERNET_PROTOCOL_ADDRESS		= $DB->query("UPDATE {$_ACCESS_DATABASE_SERVER_DATABASE_TABLE_PREFIX}_members SET
member_ip_address_authorized='000.000.000.000'
WHERE member_username='$_GLOBAL_COOKIE_MEMBER_USERNAME'
");

/*
 ============================================================================================================
 + IF: Database Server Query :: Update :: Revoke :: Registered Member Account Entry :: Internet Protocol Address :: Authorized
 ============================================================================================================
*/

if ($_Application_Module_Member_Control_Panel_DB_Query_UPDATE_REVOKE_MEMBER_ACCOUNT_AUTHORIZED_INTERNET_PROTOCOL_ADDRESS) {

echo ("Cerberus: The Current Authorized Internet Protocol Address Was Revoked Successfully.<BR>");

} else {

echo ("Cerberus: I Was Not Able To Revoke The Current Authorized Internet Protocol Address. Try Again ?<BR>");

} // [ + ] IF: Database Management System Database Server :: Query :: Update :: Revoke Authorization :: Internet Protocol Address

} // [ + ] IF: Internal Application: Display :: Authorizations :: Revoke Authorization :: IP :: Is: Activated

