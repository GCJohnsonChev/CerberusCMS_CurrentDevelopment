} else { // [ + ] IF: Safe-HTML Plugin System Setting Is: Off

	$_Application_Module_Member_Statistics_Safe_HTML_PLUGIN_STATUS	= "On";

} // [ + ] IF: Safe-HTML Plugin System Setting Is: On

