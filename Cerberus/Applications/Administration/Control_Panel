/*
 ============================================================================================================
 +
 + Administration Applications Directory
 +
 ============================================================================================================
*/

/*
 ============================================================================================================
 + Find and Open :: Directory :: Administration Applications
 ============================================================================================================
*/

$_FIND_ADMINISTRATION_APPLICATIONS_DIRECTORY			= "./Applications/Administration";
$_OPEN_ADMINISTRATION_APPLICATIONS_DIRECTORY			= opendir($_FIND_ADMINISTRATION_APPLICATIONS_DIRECTORY);

/*
 ============================================================================================================
 + WHILE: Reading :: Directory :: Administration Applications
 ============================================================================================================
*/

while (($_READ_ADMINISTRATION_APPLICATIONS_DIRECTORY = readdir($_OPEN_ADMINISTRATION_APPLICATIONS_DIRECTORY))) {

/*
 ============================================================================================================
 + IF: Stop :: Exploit :: Local-File-Inclusion and Remote-File-Inclusion
 ============================================================================================================
*/

if ($_READ_ADMINISTRATION_APPLICATIONS_DIRECTORY == "." || $_READ_ADMINISTRATION_APPLICATIONS_DIRECTORY == ".." || $_READ_ADMINISTRATION_APPLICATIONS_DIRECTORY == "index.php") {
/**
 * Do Nothing
**/
} else {

/*
 ============================================================================================================
 + IF: Internal Application :: Action :: Is: Application Module :: Administration
 ============================================================================================================
*/

if ($_GET["Action"] == "$_READ_ADMINISTRATION_APPLICATIONS_DIRECTORY") {

/*
 ============================================================================================================
 + IF: Registered Member Account :: Administrator :: Access Level :: Is: >= 3
 ============================================================================================================
*/

if ($_GLOBAL_COOKIE_MEMBER_USERNAME && $_GLOBAL_COOKIE_MEMBER_PASSWORD != null && $_GLOBAL_MEMBER_ACCESS_LEVEL >= 3) {

										include_once "$_FIND_ADMINISTRATION_APPLICATIONS_DIRECTORY/$_READ_ADMINISTRATION_APPLICATIONS_DIRECTORY";

} else {

	echo ($_Message_Kernel_APPLICATION_ADMINISTRATOR_APPLICATION_ACCESS_RESTRICTED_ADMINISTRATOR);

} // [ + ] IF: Registered Member Account :: Cookies :: Credentials :: Exist, are Valid and Registered Member Account Access-Level Is: Administrator, >= 3

} // [ + ] IF: Internal Application: Action :: Is: Activated

} // [ + ] IF: Administration Applications Directory IS Anything ELSE Except The Administration Application

} // [ + ] WHILE: Reading :: The Administration Applications Directory

/*
 ============================================================================================================
 + Close :: Directory :: Applications :: Administration
 ============================================================================================================
*/

closedir($_OPEN_ADMINISTRATION_APPLICATIONS_DIRECTORY);
